extends base

block content
  .courses
    each c in subcourses
      button.btn-clases(data-subcourseid= '')
        .first-cours
          .class-img-container
            img.first-cours-img(src=`/imgs/subcourses/${c.photoUrl}`, alt=`subcourse card`)
          .class-info 
            .asab= c.title
            .about-cours= c.description
block scripts
  script(src=`/js/subcourses.js`)